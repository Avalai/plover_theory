{
    "plugins": [
        "github-buttons", "edit-link", "page-footer-ex"
    ],
    "pluginsConfig": {
        "theme-default": {
             "showLevel": false
        },
        "github-buttons": {
          "repo": "morinted/plover_theory",
          "types": [
            "star"
          ],
          "size": "small"
        },
        "edit-link": {
            "base": "https://github.com/morinted/plover_theory/edit/master/",
            "label": "Edit This Page"
        },
        "page-footer-ex": {
            "copyright": "Plover Theory by <a href=\"http://tedmor.in\">Ted Morin</a>, licensed under <a href=\"https://creativecommons.org/licenses/by-sa/4.0/\">CC BY-SA 4.0 International</a>",
            "update_label": "last updated",
            "update_format": "YYYY-MM-DD"
        }
    }
}
