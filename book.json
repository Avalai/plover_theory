{
    "plugins": [
        "github-buttons", "edit-link", "footer"
    ],
    "pluginsConfig": {
        "theme-default": {
             "showLevel": false
        },
        "github-buttons": {
          "repo": "morinted/plover_theory",
          "types": [
            "star"
          ],
          "size": "small"
        },
        "edit-link": {
            "base": "https://github.com/morinted/plover_theory/edit/master/",
            "label": "Edit This Page"
        },
        "layout": {
            "footerPath": "footer.html"
        }
    }
}
